<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service id="liip_functional_test.database.tools.orm" class="Liip\FunctionalTestBundle\Database\Tools\ORMDatabaseTool" public="false">
            <argument type="service" id="service_container" />
            <argument type="service" id="doctrine.fixtures.loader" on-invalid="null"/>
            <tag name="liip_functional_test.database.tools"/>
        </service>

        <service id="liip_functional_test.database.tools.collection" class="Liip\FunctionalTestBundle\Database\Tools\DatabaseToolCollection" public="false">
        </service>
    </services>
</container>
